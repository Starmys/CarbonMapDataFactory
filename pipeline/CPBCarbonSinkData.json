{
	"name": "CPBCarbonSinkData",
	"properties": {
		"activities": [
			{
				"name": "Copy to SQL Carbon Sink Data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "*CarbonSinkData*.csv",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "DELETE FROM carbon_sink_data;\nCREATE TABLE carbon_sink_data\n(\n\t[Date] int NOT NULL,\n\t[Sector] int NOT NULL,\n\t[Longitude] float NOT NULL,\n\t[Latitude] float NOT NULL,\n\t[Value] float NOT NULL\n);\n",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Date",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Date",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "Sector",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Sector",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Longitude",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Longitude",
									"type": "Double",
									"physicalType": "float"
								}
							},
							{
								"source": {
									"name": "Latitude",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Latitude",
									"type": "Double",
									"physicalType": "float"
								}
							},
							{
								"source": {
									"name": "Value",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Value",
									"type": "Double",
									"physicalType": "float"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "CSVCarbonSinkData",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SQLCarbonSinkData",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}