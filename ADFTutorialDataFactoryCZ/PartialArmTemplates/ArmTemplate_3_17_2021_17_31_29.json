					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "CarbonMapPreprocess"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [],
				"typeProperties": {
					"schema": "dbo",
					"table": {
						"value": "@concat('preprocessed_data_backup_', formatDateTime(utcnow(), 'yyyy_MM_dd_hh_mm_ss'))",
						"type": "Expression"
					}
				}
			},
			"dependsOn": []
		},
